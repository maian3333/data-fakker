    <!--
        Added the constraints for entity Station.
    -->
    <changeSet id="20250923135459-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="address_id"
                                 baseTableName="station"
                                 constraintName="fk_station__address_id"
