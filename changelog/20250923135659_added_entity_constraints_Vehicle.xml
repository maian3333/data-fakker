    <!--
        Added the constraints for entity Vehicle.
    -->
    <changeSet id="20250923135659-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="seat_map_id"
                                 baseTableName="vehicle"
                                 constraintName="fk_vehicle__seat_map_id"
